<script>
                var current_song = "5";
                var song_title = "Jump then Fall";
 
  $(document).ready(function(){
 
  $("#jPlayer").jPlayer({
  ready: function () {
  playListChange("http://www.minneapolisfuckingrocks.com/mp3/taylorswift_jumpthenfall1.mp3")
  },
  ended: function (event) {
  $(this).jPlayer("play");
  },
  swfPath: "../js",
  supplied: "mp3"
  });
 
  $(".playSong").click( function() {
                                url = $(this).attr('rel');
                                song_title = $(this).attr('song_title');
                                playListChange(url);
  }); // Change to track defined by rel of link with playSong class
 
  $("#jplayer_inspector").jPlayerInspector({jPlayer:$("#jPlayer")});
 
  function playListChange(url) {
                               
                                $("#jPlayer").jPlayer("setMedia", { mp3: url}).jPlayer("play");   
                                var element = document.getElementById('player_song_name');                             
                                element.innerHTML = song_title;
                }
 
 
  })
 
 
  
  function changeStyle(elementId) {
  //alert(document.getElementById(elementId).className);
  document.getElementById(elementId).className += " current_track";
  //alert("current song is " +current_song);
  $(document.getElementById(current_song)).removeClass("current_track");
  current_song = elementId;
}
 
</script>


